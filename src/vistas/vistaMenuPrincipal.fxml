<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controladores.VistaMenuPrincipalController">
   <children>
      <MenuBar prefHeight="28.0" prefWidth="599.0">
        <menus>
          <Menu mnemonicParsing="false" text="Pacientes">
            <items>
                  <MenuItem fx:id="miPaci" mnemonicParsing="false" onAction="#eventAbrirPaci" text="irPacientes" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Médicos">
            <items>
                  <MenuItem fx:id="miMedi" mnemonicParsing="false" onAction="#eventAbrirMedi" text="irMédicos" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Historia Clinicas">
            <items>
                  <MenuItem fx:id="miHisto" mnemonicParsing="false" onAction="#eventAbrirHisto" text="irHistoria_Clinicas" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
